  m_Name: 
  m_EditorClassIdentifier: 
  sourceCodeAsset: {fileID: 0}
  sourceCode: "const body = $.subNode(\"body\");\r\nconst targetSpeed = 15;\r\n\r\nconst
    moveStraight = () => {\r\n  let dir = body.velocity.normalize();\r\n  let speed
    = targetSpeed - velocity.length();\r\n\r\n  const force = dir.multiplyScalar(speed);\r\n 
    body.addForce(force);\r\n};\r\n\r\n$.onStart(() => {\r\n  body.addForce(new Vector3(15,0,0));\r\n});\r\n\r\n$.onUpdate(deltaTime
    => {\r\n  moveStraight();\r\n});\r\n"
